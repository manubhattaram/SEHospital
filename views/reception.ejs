					<div class="panel panel-default">
							<div class="panel-heading">
								Book An Appointment

								<span class="pull-right clickable panel-toggle panel-button-tab-left"><em class="fa fa-toggle-up"></em></span></div>
							<div class="panel-body">
								<form class="form-horizontal" action="/appointment" method="post">
									<fieldset>
										<!-- Name input-->
										<div class="form-group">
											<label class="col-md-3 control-label" for="name">Patient Name</label>
											<div class="col-md-9">
												<input id="name" name="name" type="text" placeholder="Patient name" class="form-control">
											</div>
										</div>
										<div class="form-group">
											<label class="col-md-3 control-label" for="name">Patient E-mail</label>
											<div class="col-md-9">
												<input id="email" name="email" type="email" placeholder="Patient email" class="form-control">
											</div>
										</div>
										<!-- Email input-->
											<div class="form-group">
												<label class="col-md-3 control-label" for="email">Date of Birth</label>
												<div class="col-md-9">
													<input name="dob" placeholder="Date of birth"  onblur="this.placeholder = 'Enter Date of birth'"
													 class="common_input mb-10 form-control" required="" type="text" >
												</div>
											</div>

											<div class="form-group">
												<label class="col-md-3 control-label" for="email">Appointment Date</label>
												<div class="col-md-9">
													<input name="appdate" placeholder="Appointment date"  onblur="this.placeholder = 'Enter Appointment date'"
													 class="common_input mb-10 form-control" required="" type="text" onfocus="(this.type='date')" >
												</div>
											</div>
											<div class="form-group">
												<label class="col-md-3 control-label" for="email">Field of Doctor Needed</label>
												<div class="col-md-9">
							      								<!--select class="form-control">
																		<option value="1">ENT</option>
																		<option value="2">Physician</option>
																		<option value="3">Gynaecologist</option>
																		<option value="4">Dentist</option>
																	</select-->
																	<input name="field" placeholder="Field"  onblur="this.placeholder = 'ENT/Dentist/Neurology/Physician'"
													 class="common_input mb-10 form-control" required="" type="text" >
																</div>
															</div>
										<!-- Message body -->
										<div class="form-group">
											<label class="col-md-3 control-label" for="message">Message</label>
											<div class="col-md-9">
												<textarea class="form-control" id="message" name="message" placeholder="Please enter a message here..." rows="5"></textarea>
											</div>
										</div>

										<!-- Form actions -->
										<div class="form-group">
											<div class="col-md-12 widget-right">
												<button type="submit" class="btn btn-default btn-md pull-right">Submit</button>
											</div>
										</div>
									</fieldset>
								</form>
							</div>
						</div>
				</div><!--End .articles-->

